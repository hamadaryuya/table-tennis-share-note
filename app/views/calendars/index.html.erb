<%= month_calendar calendars: @trainings do |date, calendar| %>
  <%= date %>
    <% @trainings.each do |event| %>
      <% if event.date == date %>
        <%= link_to event.user.name, edit_training_path(event) %>
      <% end %>
     <% end %>
     
     <% @games.each do |game| %>
      <% if game.date == date%>
        <%= link_to game.user.name, edit_game_path(game)%>
      <% end %>
    <% end %>
<% end %>

